<h1>title search</h1>
<%= form_with url: search_posts_path, method: :get, local: true do |f| %>
  <%= f.text_field :cooking_title %>
  <%= f.submit :search %>
<% end %>
<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.cooking_title %></td>
      </tr>
    <% end %>
  </tbody>
</table>

  